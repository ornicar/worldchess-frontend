{
  "name": "worldchess-fe",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:game": "ng serve game",
    "start:stage": "ng serve --configuration=stage",
    "start:aot": "ng serve --aot",
    "build": "per-env",
    "build:dev": "node --max-old-space-size=8000 ./node_modules/.bin/ng build --configuration=dev",
    "build:stage": "node --max-old-space-size=8000 ./node_modules/.bin/ng build --configuration=stage",
    "build:prod": "node --max-old-space-size=8000 ./node_modules/.bin/ng build --prod --configuration=production",
    "build:game:dev": "node --max-old-space-size=8000 ./node_modules/.bin/ng build game --configuration=dev",
    "build:game:stage": "node --max-old-space-size=8000 ./node_modules/.bin/ng build game --configuration=stage",
    "build:game:prod": "node --max-old-space-size=8000 ./node_modules/.bin/ng build game --prod --configuration=production",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "doc:build": "compodoc -p src/tsconfig.app.json",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p src/tsconfig.app.json -s",
    "bundle-report": "ng build --prod --configuration=production --stats-json && webpack-bundle-analyzer dist/stats.json",
    "server": "nodemon --watch server.ts --exec 'ts-node' server.ts",
    "compile:server": "webpack --config src/webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run worldchess-fe:server:production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "browserslist": [
    "Edge >= 15",
    "Explorer >= 10",
    "Chrome >= 41",
    "Firefox >= 52",
    "Safari >= 10",
    "ChromeAndroid >= 60",
    "iOS >= 10"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.0.1",
    "@angular/material-moment-adapter": "^8.0.1",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/platform-server": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@ngu/carousel": "^1.5.4",
    "@nguniversal/express-engine": "^8.0.0-rc.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.0.0-rc.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/store": "^3.4.3",
    "@types/moment-duration-format": "^2.2.2",
    "@types/youtube": "0.0.35",
    "angulartics2": "^6.3.0",
    "chess.js": "^0.10.2",
    "chessground": "^7.3.2",
    "class-transformer": "^0.2.0",
    "core-js": "^2.5.4",
    "d3": "^5.5.0",
    "d3-ng2-service": "^2.2.0",
    "enhanced-resolve": "^3.3.0",
    "flag-icon-css": "^3.0.0",
    "hammerjs": "^2.0.8",
    "io-ts": "^1.3.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.2.2",
    "mousetrap": "^1.6.2",
    "ng-lottie": "0.3.1",
    "ng2-nouislider": "^1.7.12",
    "ngx-cookie": "^4.0.2",
    "ngx-image-cropper": "^1.3.8",
    "ngx-mask": "^2.7.4",
    "ngx-smart-modal": "^7.0.3",
    "ngx-virtual-scroller": "^1.0.16",
    "nouislider": "^11.1.0",
    "per-env": "^1.0.2",
    "rxjs": "^6.5.2",
    "scrolling-element": "^1.0.2",
    "tslib": "^1.9.0",
    "url-polyfill": "^1.1.3",
    "uuid": "^3.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.0.2",
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/cli": "^8.0.6",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@compodoc/compodoc": "^1.1.6",
    "@ngrx/schematics": "^7.4.0",
    "@ngxs/devtools-plugin": "^3.4.3",
    "@types/jest": "^25.2.1",
    "@types/node": "~8.9.4",
    "@types/request": "^2.48.1",
    "codelyzer": "^4.5.0",
    "dotenv": "^6.1.0",
    "fastify": "^2.4.1",
    "fastify-static": "^2.4.0",
    "jest": "^25.4.0",
    "jest-preset-angular": "^8.1.3",
    "memoizee": "^0.4.14",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.0",
    "protractor": "^5.4.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.12.1",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "websocket": "^1.0.28"
  }
}
